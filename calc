number_1 = int(input('Enter ur first number:'))
number_2 = int(input('Enter ur second number:'))
print('{} + {} = ' .format(number_1, number_2))
print(number_1 + number_2)

print('{} * {} = ' .format(number_1, number_2))
print(number_1 * number_2)

print('{} - {} =' .format(number_1, number_2))
print(number_1 - number_2)

print('{} \ {} =' .format(number_1, number_2))
print(number_1 / number_2)
